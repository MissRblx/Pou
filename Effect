--cleave text

        local cletextPart = Instance.new("Part")
        cletextPart.Name = "Text"
        cletextPart.Anchored = true
        cletextPart.CanCollide = false
        cletextPart.Transparency = 1
        cletextPart.Size = Vector3.new(0.1, 0.1, 0.1)
        cletextPart.Parent = foldercleave

        local forward = humanoidRootPart.CFrame.LookVector
        local cletextCFrame = CFrame.new(
            humanoidRootPart.Position + (forward * 5),
            humanoidRootPart.Position + (forward * 5) + forward
        )
        cletextPart.CFrame = cletextCFrame

        local function createFolderPath(parent, path)
            if not path or path == "" then return parent end
            
            local current = parent
            for _, name in ipairs(string.split(path, ".")) do
                local existing = current:FindFirstChild(name)
                if not existing then
                    local new = Instance.new("Folder")
                    new.Name = name
                    new.Parent = current
                    current = new
                else
                    current = existing
                end
            end
            return current
        end

        local particles = {}
    
            do
                local cletextticle = Instance.new("ParticleEmitter")
                local parent = cletextPart
                        cletextticle.ShapeStyle = Enum.ParticleEmitterShapeStyle.Volume
                cletextticle.FlipbookFramerate = NumberRange.new(1.000000, 1.000000)
                cletextticle.LightInfluence = 0
                cletextticle.Lifetime = NumberRange.new(1.000000, 1.000000)
                cletextticle.FlipbookLayout = Enum.ParticleFlipbookLayout.None
                cletextticle.SpreadAngle = Vector2.new(0.000000, 0.000000)
                cletextticle.LockedToPart = true
