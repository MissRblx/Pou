local foldercursedenergyName = "Cursed Energy Omaga"
local foldercursedenergy

if not game.ReplicatedStorage:FindFirstChild(foldercursedenergyName) then
    foldercursedenergy = Instance.new("Folder")
    foldercursedenergy.Name = foldercursedenergyName
    foldercursedenergy.Parent = game.ReplicatedStorage
else
    foldercursedenergy = game.ReplicatedStorage:FindFirstChild(foldercursedenergyName)
end

local cespawnPart = Instance.new("Part")
cespawnPart.Name = "Summon"
cespawnPart.Anchored = true
cespawnPart.CanCollide = false
cespawnPart.Transparency = 1
cespawnPart.Size = Vector3.new(1, 1, 1)
cespawnPart.Parent = foldercursedenergy



        local forward = humanoidRootPart.CFrame.LookVector

        local cespawnCFrame = CFrame.new(

            humanoidRootPart.Position + (forward * 5),

            humanoidRootPart.Position + (forward * 5) + forward

        )

        cespawnPart.CFrame = cespawnCFrame



        local function createFolderPath(parent, path)

            if not path or path == "" then return parent end

            

            local current = parent

            for _, name in ipairs(string.split(path, ".")) do

                local existing = current:FindFirstChild(name)

                if not existing then

                    local new = Instance.new("Attachment")

                    new.Name = name

                    new.Parent = current

                    current = new

                else

                    current = existing

                end

            end

            return current

        end



