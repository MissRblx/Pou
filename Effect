        local Players = game:GetService("Players")
        local RunService = game:GetService("RunService")

        if not RunService:IsClient() then return end

        local player = Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

local folderragingName = "Raging Dismantle"
local folderraging

if not game.ReplicatedStorage:FindFirstChild(folderragingName) then
    folderraging = Instance.new("Folder")
    folderraging.Name = folderragingName
    folderraging.Parent = game.ReplicatedStorage
else
    folderraging = game.ReplicatedStorage:FindFirstChild(folderragingName)
end

local ragdisfectPart = Instance.new("Part")
ragdisfectPart.Name = "Raging Dismantle"
ragdisfectPart.Anchored = true
ragdisfectPart.CanCollide = false
ragdisfectPart.Transparency = 1
ragdisfectPart.Size = Vector3.new(5, 5.2, 35)
ragdisfectPart.Parent = folderraging


        local forward = humanoidRootPart.CFrame.LookVector
        local ragdisfectCFrame = CFrame.new(
            humanoidRootPart.Position + (forward * 5),
            humanoidRootPart.Position + (forward * 5) + forward
        )
        ragdisfectPart.CFrame = ragdisfectCFrame

        local function createFolderPath(parent, path)
            if not path or path == "" then return parent end
            
            local current = parent
            for _, name in ipairs(string.split(path, ".")) do
                local existing = current:FindFirstChild(name)
                if not existing then
                    local new = Instance.new("Attachment")
                    new.Name = name
                    new.Parent = current
                    current = new
                else
                    current = existing
                end
            end
            return current
        end

