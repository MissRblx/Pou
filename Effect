        local mIlastfectPart = Instance.new("Part")
        mIlastfectPart.Name = "LastHit"
        mIlastfectPart.Anchored = true
        mIlastfectPart.CanCollide = false
        mIlastfectPart.Transparency = 1
        mIlastfectPart.Size = Vector3.new(0.1, 0.1, 0.1)
        mIlastfectPart.Parent = folderm1

        local forward = humanoidRootPart.CFrame.LookVector

        local mIlastfectCFrame = CFrame.new(

            humanoidRootPart.Position + (forward * 5),

            humanoidRootPart.Position + (forward * 5) + forward

        )

        mIlastfectPart.CFrame = mIlastfectCFrame



        local function createFolderPath(parent, path)

            if not path or path == "" then return parent end

            

            local current = parent

            for _, name in ipairs(string.split(path, ".")) do

                local existing = current:FindFirstChild(name)

                if not existing then

                    local new = Instance.new("Attachment")

                    new.Name = name

                    new.Parent = current

                    current = new

                else

                    current = existing

                end

            end

            return current

        end



