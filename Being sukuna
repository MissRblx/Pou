--pants

local character = game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()
local rightLeg = character:FindFirstChild("Right Leg")

if rightLeg then
    local rlegpart = Instance.new("Part")
    rlegpart.Size = Vector3.new(1, 1, 1)  -- size
    rlegpart.BrickColor = BrickColor.new("White")
    rlegpart.CanCollide = false
    rlegpart.Anchored = false
    rlegpart.Parent = workspace

    local weld = Instance.new("Weld")
    weld.Part0 = rlegpart
    weld.Part1 = rightLeg
    weld.C0 = CFrame.new(0, -0.2, 0) * CFrame.Angles(math.rad(0), 0, 0)  -- position and rotate
    weld.Parent = rlegpart

    local rightpant = Instance.new("SpecialMesh", rlegpart)
    rightpant.MeshId = "rbxassetid://140326582191138" -- correct model ID
    rightpant.TextureId = "rbxassetid://136396109135360" -- correct texture ID
    rightpant.Scale = Vector3.new(1, 1, 1)  -- scale
end

local character = game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()
local leftLeg = character:FindFirstChild("Left Leg")

if rightLeg then
    local llegpart = Instance.new("Part")
    llegpart.Size = Vector3.new(1, 1, 1)  -- size
    llegpart.BrickColor = BrickColor.new("White")
    llegpart.CanCollide = false
    llegpart.Anchored = false
    llegpart.Parent = workspace

    local weld = Instance.new("Weld")
    weld.Part0 = llegpart
    weld.Part1 = leftLeg
    weld.C0 = CFrame.new(0, -0.2, 0) * CFrame.Angles(math.rad(0), 0, 0)  -- position and rotate
    weld.Parent = llegpart

    local leftpant = Instance.new("SpecialMesh", llegpart)
    leftpant.MeshId = "rbxassetid://99772406606118" -- correct model ID
    leftpant.TextureId = "rbxassetid://136396109135360" -- correct texture ID
    leftpant.Scale = Vector3.new(1, 1, 1)  -- scale
end

